USE `test-schema`;
CREATE TABLE flight_data_table (
    s_no INT AUTO_INCREMENT NOT NULL,
    YR INT(5) NOT NULL,
    MTH INT(5) NOT NULL,
    DAY_OF_MONTH INT(10) NULL,
    FL_DATE INT(15) NULL,
    AIRLINE_ID INT(15) NULL,
    CARRIER VARCHAR(100) NULL,
    ORIGIN_AIRPORT_ID INT(15) NULL,
    ORIGIN VARCHAR(6) NULL,
    ORIGIN_CITY_NAME VARCHAR(100) COLLATE UTF8_UNICODE_CI NULL,
    ORIGIN_STATE_ABR VARCHAR(100) COLLATE UTF8_UNICODE_CI NULL,
    DEST_AIRPORT_ID INT(15) NULL,
    DEST VARCHAR(100) COLLATE UTF8_UNICODE_CI NULL,
    DEST_CITY_NAME VARCHAR(100) COLLATE UTF8_UNICODE_CI NULL,
    DEST_STATE_ABR VARCHAR(100) COLLATE UTF8_UNICODE_CI NULL,
    DEP_TIME INT(15) NULL,
    DEP_DELAY INT(15) NULL,
    DEP_DELAY_NEW INT(15) NULL,
    ARR_TIME INT(15) NULL,
    ARR_DELAY INT(15) NULL,
    ARR_DELAY_NEW INT(15) NULL,
    CANCELLED INT(15) NULL,
    AIR_TIME INT(15) NULL,
    FLIGHTS VARCHAR(15) COLLATE UTF8_UNICODE_CI NULL,
    DISTANCE VARCHAR(10) COLLATE UTF8_UNICODE_CI NULL,
    PRIMARY KEY (s_no)
);
